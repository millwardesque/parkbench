# This file contains required environment variables for the Parkbench application.
# Copy this file to .env for local development and fill in the appropriate values.

# Database connection string (generated by `prisma init`)
# Example: DATABASE_URL="postgresql://user:password@localhost:5432/parkbench?schema=public"
DATABASE_URL=""

# Email service provider. Use 'console' for local development to log emails.
# For production, this could be 'resend', 'postmark', etc., and would require an API key.
EMAIL_PROVIDER=console

# Resend API key (for production)
RESEND_API_KEY=""

# Session secret. Required for session management.
SESSION_SECRET=""